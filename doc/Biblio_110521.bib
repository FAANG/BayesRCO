	month = sep,
	year = {2019},
	pages = {19398--19408},
	file = {Xiang et al. - 2019 - Quantifying the contribution of sequence variants .pdf:C\:\\Users\\fmollandin\\Zotero\\storage\\TILM9FR7\\Xiang et al. - 2019 - Quantifying the contribution of sequence variants .pdf:application/pdf}
