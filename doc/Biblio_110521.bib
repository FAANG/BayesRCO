	journal = {PLOS Computational Biology},
	author = {Liu, Wei and Li, Mo and Zhang, Wenfeng and Zhou, Geyu and Wu, Xing and Wang, Jiawei and Lu, Qiongshi and Zhao, Hongyu},
	editor = {Kim, Seyoung},
	month = nov,
	year = {2020},
	pages = {e1008315},
	file = {Liu et al. - 2020 - Leveraging functional annotation to identify genes.pdf:C\:\\Users\\fmollandin\\Zotero\\storage\\N6LCY6JA\\Liu et al. - 2020 - Leveraging functional annotation to identify genes.pdf:application/pdf}
