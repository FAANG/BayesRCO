	number = {5},
	urldate = {2020-02-28},
	journal = {PLOS ONE},
	author = {Zhu, Bo and Zhu, Miao and Jiang, Jicai and Niu, Hong and Wang, Yanhui and Wu, Yang and Xu, Lingyang and Chen, Yan and Zhang, Lupei and Gao, Xue and Gao, Huijiang and Liu, Jianfeng and Li, Junya},
	editor = {Zhao, Shuhong},
